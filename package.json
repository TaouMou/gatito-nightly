{
  "name": "gatito-nightly",
  "displayName": "Gatito Nightly",
  "description": "Fork of Gatito Theme with darker background and more use of accent color, and another theme with Nord colors.",
  "version": "1.2.2",
  "publisher": "TaouMou",
  "license": "MIT",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Themes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TaouMou/gatito-nightly"
  },
  "keywords": [
    "Theme",
    "Dark theme",
    "Gatito",
    "Nord",
    "Darker Nord",
    "Nordic",
    "Nightly",
    "Taoumou",
    "Dark",
    "Gatito dark"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Gatito Nightly",
        "uiTheme": "vs-dark",
        "path": "./themes/gatitio-color-theme.json"
      },
      {
        "label": "Gatito Nightly Blue",
        "uiTheme": "vs-dark",
        "path": "./themes/gatito-blue-color-theme.json"
      }
    ],
    "scripts": {
      "package": "vsce package",
      "start": "npm run build; npm run package",
      "publish": "vsce publish"
    },
    "devDependencies": {
      "vsce": "^1.96.1"
    }
  }
}
